{
  "name": "@emmraan/ai-skills",
  "version": "1.0.2",
  "description": "CLI tool for installing AI Skills across agent platforms",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Emmraan/ai-skills.git"
  },
  "homepage": "https://github.com/Emmraan/ai-skills#readme",
  "bugs": {
    "url": "https://github.com/Emmraan/ai-skills/issues"
  },
  "bin": {
    "ai-skills": "bin/skills.js",
    "skills": "bin/skills.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "tsx src/bin/skills.ts",
    "test": "vitest run",
    "lint": "node -e \"process.exit(0)\"",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "inquirer": "^12.9.6",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@ai-skills/shared-types": "workspace:*",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "vitest": "^1.1.0"
  }
}
